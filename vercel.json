{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key",
      "OTHER_ENV_VARIABLE": "@other_env_variable"
    }
  }
  